<h1>Admin#login</h1>


<h4><%= flash[:error] %></h4>
<%= form_with do |form| %>

 <div class="field">
    <%= form.label :username %> 
    <%= form.text_field :username %>
</div>

 <div class="field">
    <%= form.label :password %> 
    <%= form.password_field :password %>
</div>

<div class="field">
    <%= form.label "Remeber me" %> 
    <%= form.check_box :remember_me %> <br />
</div>

<div class="actions">
    <%= form.submit %> 
</div>
<% end %>